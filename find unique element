#include<bits/stdc++.h>
int findUnique(int *arr, int size)
{
    int n= size;
    for(int i =0; i<n; i++){
        int j =0;
        for(; j<n; j++){
            if(i!=j && arr[i]== arr[j]){
                
                break;
            }
        }
        if(j== n ){
            return arr[i];
        }
    }
}
               
           
